{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>shun-shobon/renovate-config", ":dependencyDashboard"],
  "regexManagers": [
    {
      "fileMatch": ["\\.tsx?$"],
      "matchStrings": [
        "(?:im|ex)port(?:.|\\s)+?from\\s*['\"](?<depName>https://deno.land/.+?)@(?<currentValue>\\d+?\\.\\d+?\\.\\d+?)/.*?['\"]"
      ],
      "datasourceTemplate": "deno"
    }
  ]
}
